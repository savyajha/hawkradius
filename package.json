{
  "name": "gatsby-starter-try-ghost",
  "description": "Turn your Ghost blog into a flaring fast static site with Gatsby",
  "version": "1.19.2",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-plugin",
    "jamify",
    "ghost",
    "ghost-theme",
    "casper",
    "blogging",
    "static-site",
    "static-site-generator",
    "gatsbyjs",
    "react"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "config": "git update-index --skip-worktree .ghost.json",
    "deploy": "gatsby build && echo 'demo.jamify.org' > public/CNAME && gh-pages -f -d public -b master -r git@github.com:styxlab/styxlab.github.io"
  },
  "dependencies": {
    "gatsby": "2.21.1",
    "gatsby-plugin-ackee-tracker": "^1.4.0",
    "gatsby-rehype-ghost-links": "^1.3.1",
    "gatsby-rehype-prismjs": "^1.0.0",
    "gatsby-theme-ghost-dark-mode": "^1.3.0",
    "gatsby-theme-ghost-members": "^1.2.1",
    "gatsby-theme-try-ghost": "^1.19.2",
    "gatsby-transformer-rehype": "^1.4.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-plugin-ghost": "^1.5.0",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
